{
  "version": 2,
  "builds": [{ "src": "*.js", "use": "@vercel/node" }],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/",
      "headers" : {
        "Access-Control-Allow-Origin": "*, https://next-quiz-bice.vercel.app/",
        "Access-Control-Allow-Methods": "GET,POST,PUT,DELETE",
        "Access-Control-Allow-Headers": "Content-Type, Origin, Authorization"
      }
    }
  ]
}